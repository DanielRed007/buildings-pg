{
  "name": "buildings-pg",
  "version": "1.0.0",
  "description": "A NodeJS API REST example using PostgresSQL",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "migrate": "npx sequelize db:migrate --url 'postgres://pgbuilder:pgbuilder@localhost:5432/buildings_pg'",
    "seed": "sequelize db:seed:all",
    "undo-migrate": "sequelize db:migrate:undo",
    "undo-seed": "sequelize db:seed:undo:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/sequelize": "^4.28.19",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "sequelize-typescript": "^2.1.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
